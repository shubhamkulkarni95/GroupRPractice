package org.string;

public class StringRev {

	public static void main(String[] args) {
		String str="Hello My name is shubham";
		String[] str1=str.split("");
		for(int i=str1.length-1;i>=0;i--) {
			if(str1[i]==str1[str1.length-1]) {
				str1[i].toUpperCase();
			}
			System.out.print(str1[i]);
			}
		
	}

}
